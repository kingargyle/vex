<!ELEMENT title (#PCDATA)>
<!ELEMENT any ANY>
<!ELEMENT empty EMPTY>
<!ELEMENT section (title?, para+)>
<!ELEMENT para (#PCDATA | emphasis)*>
<!ELEMENT emphasis (#PCDATA)>
<!ELEMENT one-kind-of-child (section+)>
<!ELEMENT document (title?, preface, section+, index)>
<!ELEMENT preface (para+)>
<!ELEMENT index (para+)>

